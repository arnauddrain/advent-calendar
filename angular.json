{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "calendar": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/calendar/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            },
            "preview": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.preview.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            },
            "development": {
              "optimization": {
                "scripts": false,
                "styles": false,
                "fonts": false
              }
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "calendar:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "calendar:build:production"
            },
            "preview": {
              "browserTarget": "calendar:build:preview"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "calendar:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": true,
              "extractLicenses": true
            },
            "preview": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.preview.ts"
                }
              ],
              "optimization": true,
              "sourceMap": true,
              "extractLicenses": true
            },
            "development": {
              "optimization": true
            }
          },
          "defaultConfiguration": "production",
          "server": {
            "builder": "@angular-devkit/build-angular:server",
            "options": {
              "outputPath": "dist/calendar/server",
              "main": "server.ts",
              "tsConfig": "tsconfig.server.json"
            },
            "configurations": {
              "production": {
                "outputHashing": "none",
                "fileReplacements": [
                  {
                    "replace": "src/environments/environment.ts",
                    "with": "src/environments/environment.prod.ts"
                  }
                ],
                "optimization": true,
                "sourceMap": true,
                "extractLicenses": true
              },
              "preview": {
                "outputHashing": "none",
                "fileReplacements": [
                  {
                    "replace": "src/environments/environment.ts",
                    "with": "src/environments/environment.preview.ts"
                  }
                ],
                "optimization": true,
                "sourceMap": true,
                "extractLicenses": true
              },
              "development": {
                "optimization": true
              }
            },
            "defaultConfiguration": "production"
          },
          "serve-ssr": {
            "builder": "@nguniversal/builders:ssr-dev-server",
            "configurations": {
              "development": {
                "browserTarget": "calendar:build:development",
                "serverTarget": "calendar:server:development"
              },
              "production": {
                "browserTarget": "calendar:build:production",
                "serverTarget": "calendar:server:production"
              }
            },
            "defaultConfiguration": "development"
          },
          "prerender": {
            "builder": "@nguniversal/builders:prerender",
            "options": {
              "routes": [
                "/"
              ]
            },
            "configurations": {
              "production": {
                "browserTarget": "calendar:build:production",
                "serverTarget": "calendar:server:production"
              },
              "development": {
                "browserTarget": "calendar:build:development",
                "serverTarget": "calendar:server:development"
              }
            },
            "defaultConfiguration": "production"
          }
        }
      }
    }
  },
  "defaultProject": "calendar",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
